{
  "expo": {
    "name": "Patient Dashboard Pro",
    "slug": "patient-dashboard-pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "patientdashboard",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "62184485-4a55-410e-a8e0-8a73a6a6b3cd"
      }
    },
    "android": {
      "package": "com.rohit_kumar2001.patientdashboardpro"
    }
  }
}
